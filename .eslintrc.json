{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "react/no-array-index-key": "off",
    "react/jsx-wrap-multilines": "off",
    "react/jsx-one-expression-per-line": "off",
    "prettier/prettier": ["error"]
  },
  "env": {
    "browser": true
  },
  "parser": "babel-eslint"
}
